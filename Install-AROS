(welcome)
(set @default-dest "LIBS:")
(copylib
  (prompt "Copying filesysbox.library")
  (help @copylib-help)
  (source "Libs/filesysbox.library")
  (dest @default-dest)
)
(copylib
  (prompt "Copying FbxDismount command")
  (help @copylib-help)
  (source "C/FbxDismount")
  (dest "C:")
)
(protect "C:FbxDismount" "+p")
(exit)
